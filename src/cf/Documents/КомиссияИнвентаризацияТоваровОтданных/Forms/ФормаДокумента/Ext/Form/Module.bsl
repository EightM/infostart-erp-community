&НаСервере
Функция ОпределитьУчетноеКоличество(Номенклатура, ХарактеристикаНоменклатуры, СерияНоменклатуры, Коэффициент)
	Если НЕ ЗначениеЗаполнено(Номенклатура) Тогда Возврат 0; КонецЕсли;
	Если НЕ ЗначениеЗаполнено(Коэффициент ) Тогда Возврат 0; КонецЕсли;

	Запрос=Новый Запрос;
	Запрос.УстановитьПараметр("ДоговорКонтрагента", Объект.ДоговорКонтрагента);
	Запрос.УстановитьПараметр("ДокументСсылка",     Объект.Ссылка);
	Запрос.УстановитьПараметр("Товар",              Номенклатура);
	Запрос.УстановитьПараметр("Характеристика",     ХарактеристикаНоменклатуры);
	Запрос.УстановитьПараметр("Серия",              СерияНоменклатуры);
	Запрос.УстановитьПараметр("Сделка",             Объект.Сделка);
	Запрос.Текст="
	|ВЫБРАТЬ
	|	ОстаткиТоваров.КоличествоОстаток КАК Количество
	|ИЗ
	|	РегистрНакопления.ТоварыПереданные.Остатки(, Номенклатура = &Товар
	|	" + ?(НЕ ЗначениеЗаполнено(ХарактеристикаНоменклатуры),"","
	|	              И ХарактеристикаНоменклатуры = &Характеристика") + "
	|	" + ?(НЕ ЗначениеЗаполнено(Объект.Сделка),"","
	|	              И Сделка = &Сделка") + "
	|	              И ДоговорКонтрагента = &ДоговорКонтрагента) КАК ОстаткиТоваров
	|ИТОГИ
	|	Сумма(Количество)
	|ПО
	|	ОБЩИЕ
	|";
	Выборка=Запрос.Выполнить().Выбрать();
	Если Выборка.Количество()=0 Тогда Возврат 0; КонецЕсли;

	Если Выборка.Следующий() Тогда
		Если НЕ ЗначениеЗаполнено(Выборка.Количество) Тогда Возврат 0; КонецЕсли;
		Возврат Выборка.Количество * Номенклатура.ЕдиницаХраненияОстатков.Коэффициент / Коэффициент;
	КонецЕсли;

	Возврат 0;
КонецФункции

